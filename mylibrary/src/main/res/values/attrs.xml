<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="RoundImageView">
        <attr name="img_radius" format="dimension" />
        <attr name="img_fitxy" format="boolean" />
        <attr name="img_leftTopRadius" format="dimension" />
        <attr name="img_leftBottomRadius" format="dimension" />
        <attr name="img_rightTopRadius" format="dimension" />
        <attr name="img_rightBottomRadius" format="dimension" />
        <attr name="img_type">
            <flag name="circle" value="0"/>
            <flag name="round" value="1"/>
        </attr>
        <attr name="img_aspectRatio" format="float" />
    </declare-styleable>
</resources>